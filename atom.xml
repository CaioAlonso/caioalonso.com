---
layout: null
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Caio Alonso</title>
  <link href="http://caioalonso.com/"/>
  <link type="application/atom+xml" rel="self" href="http://caioalonso.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://caioalonso.com/</id>
  <author>
    <name>Caio Alonso</name>
    <email>caio@caioalonso.com</email>
  </author>

  {% for post in site.posts limit:10 %}
  <entry>
    <id>http://caioalonso.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://caioalonso.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Your Name</name>
      <uri>http://caioalonso.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
